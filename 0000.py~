INSTRUCTION_FORMAT = "A"

ASM_NAME = "Result"

def commandExec(param):
    if (param[1] == 1):
        emu.storeWord(10, emu.loadReg(3))
    else if (param[1] == 2):
        emu.storeWord(10, emu.loadReg(3) - 1)
    else if (param[3] == 1):
        emu.storeWord(10 , -1)
    emu.incrementProgramCounter(1)
